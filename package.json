{
  "name": "planning-poker",
  "version": "0.0.1",
  "description": "Sprint planning tool to make agile estimating and planning faster and more efficient",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test babel-node node_modules/mocha/bin/mocha src --recursive --full-trace",
    "test:watch": "npm run test -- -w",
    "coverage": "NODE_ENV=test babel-node node_modules/isparta/bin/isparta cover _mocha -- src --recursive",
    "start": "NODE_ENV=development DEBUG=socket.io* nodemon src/index.js",
    "build": "NODE_ENV=production webpack"
  },
  "engines": {
    "node": "4.2.x",
    "npm": "3.4.x"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/chrisandrews7/planning-poker.git"
  },
  "author": "Chris Andrews",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "config": "^1.19.0",
    "express": "^4.13.3",
    "helmet": "^1.1.0",
    "redis": "^2.4.2",
    "socket.io": "^1.4.4",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "faker": "^3.0.1",
    "http-status": "^0.2.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.3",
    "socket.io-client": "^1.4.5",
    "supertest": "^1.1.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
