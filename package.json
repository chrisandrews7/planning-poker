    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^2.4.0",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
